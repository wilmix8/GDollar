for (int i=0;i<=1000;i++)
ar.add(i);
root.add("wilmix");
root.add("jemin");
root.add("shalom");
root.add("1010");
root.add("101");
root.add("201");
root.add(ar.StringConvert());
root.add("100000000");
//print the tree's size and contents
Print.Println( "\n\n",root.size() );
root.printTree();
}
}
</Statements>
