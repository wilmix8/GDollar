while (it.<HAS>)
{
<OBJECT> el= it.next();
Print.Println("/n",el.StringConvert());
}
<SBD> sb = <NEW> <SBD>("weldone wilmix");
int a;
a=105;
if (( a >100) AND (a<=106))
GDOLLAR.WriteLine("/n"+a);
if (a NOT= 0)
GDOLLAR.WriteLine("/n"+a);
a+=2+counter;
if (a # 107)
GDOLLAR.WriteLine("/n"+a);
